{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^check", "^build"],
      "outputs": ["dist/**"]
    },
    "check": {},
    "dev:cjs": {
      "cache": false
    },
    "dev:esm": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["dev:cjs", "dev:esm"],
      "cache": false
    }
  },
  "globalDependencies": ["tsconfig.base.json", "tsconfig.json"]
}
