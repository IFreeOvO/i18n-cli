{
  "name": "@ifreeovo/i18n-extract-cli",
  "version": "1.0.1",
  "description": "自动将中文替换成i18n国际化标记的脚手架工具",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "it": "bin/index.js"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "rimraf dist && tsc --build"
  },
  "keywords": [
    "i18n",
    "intl",
    "extract",
    "cli"
  ],
  "engines": {
    "node": ">=16"
