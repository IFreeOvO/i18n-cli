    "@babel/template": "^7.18.10",
    "@babel/traverse": "^7.20.0",
    "@babel/types": "^7.20.0",
    "chalk": "4.1.2",
    "commander": "^9.4.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "leven": "3.1.0",
    "lodash": "^4.17.21",
    "semver": "^7.3.8",
    "slash": "3.0.0"
